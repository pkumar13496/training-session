<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DOBValidationRule</fullName>
    <active>true</active>
    <errorConditionFormula>Date_Of_Brith__c = TODAY()</errorConditionFormula>
    <errorMessage>To avoid DOB equal to TODAY!!!</errorMessage>
</ValidationRule>
